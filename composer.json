{
    "name": "spatie/laravel-log-dumper",
    "description": "A function to dump anything to the log",
    "keywords": [
        "spatie",
        "laravel-log-dumper"
    ],
    "homepage": "https://github.com/spatie/laravel-log-dumper",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.72|^9.0",
        "illuminate/support": "^8.72|^9.0"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^6.23|^7.0",
        "phpunit/phpunit": "^9.4",
        "spatie/phpunit-snapshot-assertions": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LogDumper\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LogDumper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\LogDumper\\LogDumperServiceProvider"
            ]
        }
    }
}
